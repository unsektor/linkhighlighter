{
  "name": "linkhighlighter",
  "version": "1.2.2",
  "description": "Tiny library to highlight active link on web page",
  "homepage": "https://проекты.unsektor.com/linkhighlighter.js/",
  "scripts": {
    "build": "yarn run clean && webpack --mode=production",
    "dev": "yarn run clean && webpack --mode=development --watch",
    "build:dev": "yarn run clean && webpack --mode=development",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "author": "Михаил Драгункин <contact@md.land>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/unsektor/linkhighlighter/"
  },
  "keywords": [
    "highlight",
    "current",
    "active",
    "url",
    "uri",
    "library",
    "universal",
    "umd",
    "esm",
    "commonjs"
  ],
  "main": "dist/linkhighlighter.umd.js",
  "module": "dist/linkhighlighter.esm.js",
  "browser": "dist/linkhighlighter.umd.js",
  "unpkg": "dist/linkhighlighter.umd.js",
  "jsdelivr": "dist/linkhighlighter.umd.js",
  "exports": {
    ".": {
      "import": "./dist/linkhighlighter.esm.js",
      "require": "./dist/linkhighlighter.umd.js",
      "browser": "./dist/linkhighlighter.umd.js",
      "default": "./dist/linkhighlighter.umd.js"
    }
  },
  "types": "dist/linkhighlighter.d.ts",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  },
  "files": [
    "dist",
    "license.md",
    "readme.md"
  ],
  "dependencies": {}
}
